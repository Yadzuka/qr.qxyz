<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">

    <display-name>Test</display-name>

    <welcome-file-list>
        <welcome-file>members.jsp</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>index.htm</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>engineServlet</servlet-name>
        <servlet-class>org.eustrosoft.pack.EngineServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>engineServlet</servlet-name>
        <url-pattern>/engine</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>qrServlet</servlet-name>
        <servlet-class>org.eustrosoft.pack.QRcodeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>qrServlet</servlet-name>
        <url-pattern>/engine/qr</url-pattern>
    </servlet-mapping>
    
        <servlet>
        <servlet-name>contactServlet</servlet-name>
        <servlet-class>org.eustrosoft.contractpkg.ContractServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>contractServlet</servlet-name>
        <url-pattern>/contractServlet</url-pattern>
    </servlet-mapping>

</web-app>
